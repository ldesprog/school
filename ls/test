#include <errno.h>
#include <string.h>
#include <stdio.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <unistd.h>
#include <dirent.h>

int main()
{
	printf("EACCES: %s\n", strerror(EACCES));
	printf("EBADF: %s\n", strerror(EBADF));
	printf("EMFILE: %s\n", strerror(EMFILE));
	printf("ENFILE: %s\n", strerror(ENFILE));
	printf("ENOENT: %s\n", strerror(ENOENT));
	printf("ENOMEM: %s\n", strerror(ENOMEM));
	printf("ENOTDIR: %s\n", strerror(ENOTDIR));
	return 0;
}